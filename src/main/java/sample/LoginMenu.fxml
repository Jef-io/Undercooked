<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1100.0" style="-fx-background-color: #737373;" stylesheets="@mainStyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginMenuController">
      <Pane prefHeight="800.0" prefWidth="1100.0" style="-fx-background-color: #737373;">
            <Text fx:id="username" fill="#ffd84d" layoutX="285.0" layoutY="149.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username :" textAlignment="CENTER" wrappingWidth="530.0">
               <font>
                  <Font name="System Bold" size="50.0" />
               </font>
            </Text>
            <Text fx:id="password" fill="#ffd84d" layoutX="284.0" layoutY="366.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password :" textAlignment="CENTER" wrappingWidth="530.0">
               <font>
                  <Font name="System Bold" size="50.0" />
               </font>
            </Text>
            <Button fx:id="loginSignIn" layoutX="284.0" layoutY="590.0" mnemonicParsing="false" onMouseClicked="#login" prefHeight="81.0" prefWidth="530.0" style="-fx-background-color: #ffffff; -fx-padding: 0;" text="LOG IN">
               <font>
                  <Font size="50.0" />
               </font>
            </Button>
            <Button fx:id="goBack" layoutX="38.0" layoutY="23.0" mnemonicParsing="false" onMouseClicked="#goBack" prefHeight="103.0" prefWidth="109.0" style="-fx-background-color: #737373; -fx-padding: 0;" text="&lt; - -" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="48.0" />
               </font>
            </Button>
            <Button fx:id="changeLoginSignInButton" layoutX="266.0" layoutY="696.0" mnemonicParsing="false" onMouseClicked="#changeLoginSignIn" prefHeight="54.0" prefWidth="567.0" style="-fx-background-color: #737373; -fx-padding: 0;" text="No account ? Sign in" textFill="#ffd84d">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <TextField fx:id="userText" layoutX="284.0" layoutY="179.0" prefHeight="59.0" prefWidth="530.0" />
            <PasswordField fx:id="passwordText" layoutX="284.0" layoutY="400.0" prefHeight="59.0" prefWidth="530.0" />
      </Pane>
</AnchorPane>
